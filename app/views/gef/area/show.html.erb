<main class="protected-areas">

  <div class="container">
    <h1><img src="/images/gef_logo.png" /> Protected Planet Tool</h1>
  </div>

  <div class="col-xs-9">
    <h2>GEF ID #<%= @gef_area[:gef_pmis_id] %></h2>
  </div>

  <div class="col-xs-6">
    <table class="table-condensed">
      <tr>
        <th>WDPA ID</td>
        <th>WDPA Name</td>
        <th>PAME Page</td>
      </tr>
      <% @protected_areas.each do |pa| %>
        <tr>
          <td><%= pa[:wdpa_id] %></td>
          <td><%= link_to 'Link', @protected_planet_link %></td>
          <td><%= link_to pa[:wdpa_data][:name], path_to_pame_records %></td>
        </tr>
      <% end %>
    </table>
  </div>
</main>
